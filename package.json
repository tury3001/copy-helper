{
  "name": "copy-helper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build-css": "tailwindcss build style.css -o public/css/style.css"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "path": "^0.12.7",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^8.7.1",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.19",
    "typescript": "~3.9.3",
    "vite": "^2.5.4",
    "vue-jest": "^5.0.0-0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
